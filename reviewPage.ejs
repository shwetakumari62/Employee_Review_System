<link rel="stylesheet" href="/css/reviewPage.css" type="text/css" />



<section id="review-sec">
    <ul class="review-list" >
        <div class="headings">
            <h1>Feedback</h1>
            <h1>Reviewer</h1>
            <h1>Reviewee</h1>
            <h1>Status</h1>
            

        </div>
        <% for(i of allReviews){ %>
            <div >
                <% if(!i.feedback){ %>
                    <p>needs to be reviewed</p>
                    <p><%= i.reviewer._id %></p>
                    <p><%= i.tobereviewed._id %></p>
                    <img he src="/images/pending-removebg-preview.png" alt="">

                <% }else{ %>
                    <p><%= i.feedback %></p>
                    <p><%= i.reviewer._id %></p>
                    <p><%= i.tobereviewed._id %></p>
                    <img src="https://cdn-icons-png.flaticon.com/128/1365/1365307.png" alt="">
                    
                <% } %>
            </div>

        <% } %>
    </ul>
</section>